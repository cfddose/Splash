FoamFile
{
    format      ascii;
    class       dictionary;
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

defaultFieldValues
(
    volScalarFieldValue alpha.ammonia 0.999999999    
);

regions
(
    // Setting values inside a box
    boxToCell
    {
        box     (-1e5 -1e5 -1e5) (1e5 1e5 1e5);
        fieldValues
        (
            volScalarFieldValue alpha.ammonia 0.999999999            
        );
    }
    
    boxToFace
    {
        box     (-1e5 -1e5 -1e5) (1e5 1e5 1e5);
        fieldValues
        (
            volScalarFieldValue alpha.ammonia 0.999999999            
        );
    }
    
);


